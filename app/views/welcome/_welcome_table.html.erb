<% content_for :main_region do %>

<br/>
<TABLE id="main-table">
<% ar = MarketingContextType.all%>
<%   entry = 0 ; row = 0; while(true) do %>
     <TR>
<%   (0..4).each do %>
<%     if entry < ar.length %>
<%       if ar[entry].name != 'welcome' %>
           <TD class='transparent-class' ><%= link_to(ar[entry].best_prompt,goto_topic_path(ar[entry].name.gsub(' ','_').downcase))%></TD>
<%       end %>
<%       entry += 1 %>
<%     end %>
<%   end %>
     </TR>
<%   break if entry > ar.length-1 %>
<% end %>
</TABLE>
<% end %>
