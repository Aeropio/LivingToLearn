<% context = @goto_topic_symbol %>
<% if user_signed_in? and current_user.marketing_contexts.length>0%>
<%   context ||= current_user.current_marketing_context_type.name %>
<% end%>
<% if context%>
  <% case context.downcase.gsub(' ','_') %>
         <% when "pto_officer"%>
           <%= render 'pto_officer_table'%>
         <% when "superintendent" %>
           <%= render 'superintendent_table'%>
         <% when "principal" %>
           <%= render 'principal_table'%>
         <% when "curriculum_director" %>
           <%= render 'curriculum_director_table'%>
         <% when "student" %>
           <%= render 'student_table'%>
        <% when "parent" %>
           <%= render 'parent_table'%>
        <% when "business" %>
           <%= render 'business_table'%>
        <% when "non_profit" %>
           <%= render 'non_profit_table'%>
        <% when "government" %>
           <%= render 'government_table'%>
        <% when "personality" %>
           <%= render 'personality_table'%>
        <% when "banker" %>
           <%= render 'banker_table'%>
  <% end %>
<% else %>
  <%= render 'welcome_table'%>
<% end %>